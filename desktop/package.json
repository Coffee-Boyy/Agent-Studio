{
  "name": "desktop",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "electron/main.mjs",
  "scripts": {
    "dev": "concurrently -k \"vite --host 127.0.0.1 --port 5173\" \"wait-on http-get://localhost:5173 && cross-env ELECTRON_RENDERER_URL=http://localhost:5173 VITE_DEV_SERVER_URL=http://localhost:5173 pnpm exec electron .\"",
    "build": "tsc && vite build",
    "build:app": "pnpm build && electron-builder",
    "preview": "vite preview --port 5173"
  },
  "dependencies": {
    "@xyflow/react": "^12.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.6.2",
    "vite": "^7.3.1",
    "wait-on": "^8.0.4"
  },
  "build": {
    "appId": "com.agentstudio.desktop",
    "productName": "Agent Studio",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**",
      "electron/**",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  }
}
